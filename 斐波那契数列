class Fib(object):

    def __init__(self, num):
        a,b,l=0,1,[]

        
        for i in range(num):
            l.append(a)
            a,b=b,a+b
        self.numbers=l   
    def __str__(self):
        return str(self.numbers)
    __repr__=__str__
    def __len__(self):
        return len(self.numbers)

f = Fib(10)
print f
print len(f)
